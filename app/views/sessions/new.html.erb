<h1>ログイン</h1>

<div>
  <%= form_with(url: sessions_path, scope: :session, local: true) do |f| %>
    <%= f.label :email, 'メール' %>
    <%= f.email_field :email %>
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password %>
    
    <%= f.submit 'ログイン' %>
  <% end %>
</div>

<%= link_to 'ユーザー登録', new_user_path %>
